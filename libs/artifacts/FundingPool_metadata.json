			"methods": {
				"calculateExpectedPayout(uint256,address,uint256)": {
					"params": {
						"investor": "Investor address",
						"repaymentAmount": "Total repayment amount including yield",
						"tokenId": "Invoice NFT token ID"
					},
					"returns": {
